/**index.wxss**/
.container-index {
  background: #f5f5f5;
}

.header-content {
  position: fixed;
  width: 100%;
  height: 44px;
  line-height: 44px;
  text-align: left;
  top: 0;
  background-color: antiquewhite;
  padding-left: 30rpx;
  z-index: 1;
}

.header-nav {
  width: 100%;
  overflow: hidden;
  background-color: antiquewhite;
}
.nav-block {
  width: 100%;
  margin-top: 20rpx;
  border-top-right-radius: 10%;
  border-top-left-radius: 10%;
  background-image: linear-gradient(white, #f5f5f5);
  overflow: hidden;
}
.nav-search {
  margin-top: 20rpx;
  margin-left: 4vw;
  padding: 10rpx;
  width: 90vw;
  height: 44rpx;
  /* border: 1px solid #e9e9e9; */
  background-color: #e9e9e9;
  border-radius: 30rpx;
}
.box-margin {
  margin: 0 24rpx;
}









.userinfo-nickname {
  color: #aaa;
}
