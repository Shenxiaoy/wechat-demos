page {
  background-color: #eeeeee;
}
.box {
  font-size: 13px;
}
.header-info {
  width: 100%;
  height: 100rpx;
  display: flex;
  padding: 24rpx;
  background-color: #faebd7;
}
.header-image {
  width: 100rpx;
  height: 100rpx;
  margin-left: 12rpx;
  border-radius: 50%;
}
.header-desc {
  margin-left: 12rpx;
  height: 100rpx;
  display: flex;
  align-items: center;
}
.info-revise {
  margin-left: 10rpx;
  display: inline-block;
  padding: 2rpx 12rpx;
  background-color: white;
  border-radius: 35rpx;
  font-size: 10px;
}
.font-whrite {
  width: 10px;
  height: 10px;
}

.content-list {
  width: 100%;
  margin-top: 12rpx;
  background-color: white;
}

.info-list {
  padding: 12rpx 0;
}
.list-item-icon {
  width: 10px;
  height: 10px;
}
.list-item {
  display: flex;
  padding: 12rpx 0;
  line-height: 50rpx;
  border-bottom: 1px solid #eeeeee;
}
.info-list .list-item:last-child {
  border-bottom: none;
}